{   
    "env": {
        "browser": true,
        "es6": true
    },

    "extends": [
        "defaults/configurations/eslint.js"
    ],
    
    "globals": {
        "$": true,
        "this": true,
        "Flickr": true
    },

    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console:": 0
            
    }
}